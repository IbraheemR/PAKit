{
  "name": "pakit",
  "version": "1.0.0",
  "description": "Pack and unpack starbound's .pak files",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . PAKit --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . PAKit --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"PAKit\"",
    "package-linux": "electron-packager . PAKit --overwrite --asar --platform=linux --arch=x64  --prune=true --out=release-builds"
  },
  "author": "OBAN (Ibraheem Rodrigues)",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "bluebird": "^3.5.1",
    "electron-packager": "^11.0.1",
    "electron-store": "^1.3.0",
    "env-paths": "^1.0.0",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "electron": "^1.8.2"
  }
}